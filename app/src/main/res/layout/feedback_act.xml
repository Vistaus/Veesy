<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/feedback_imgV"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:contentDescription="@string/dataTransmissionSuccess"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="34dp"
        android:src="@drawable/accept_deny_dialog_negative_bg" />

    <TextView
        android:id="@+id/feedback_textV"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_below="@+id/feedback_imgV"
        android:layout_centerHorizontal="true"
        />

    <ImageButton
        android:id="@+id/bDetails"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_below="@+id/feedback_textV"
        android:layout_toStartOf="@+id/feedback_textV"
        android:contentDescription="@string/details"
        android:onClick="bDetailsClicked"
        android:src="@android:drawable/ic_menu_info_details" />

    <ImageButton
        android:id="@+id/bHome"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_below="@+id/feedback_textV"
        android:layout_toEndOf="@+id/feedback_textV"
        android:contentDescription="@string/home"
        android:onClick="bHomeClicked"
        android:src="@mipmap/ic_launcher" />


</RelativeLayout>